{
  "extends": ["html-validate:recommended"],
  "rules": {
    "no-trailing-whitespace": "off",
    "void-style": ["error", { "style": "selfclosing" }],
    "no-inline-style": "warn",
    "require-sri": "off",
    "long-title": "warn",
    "no-raw-characters": "warn",
    "wcag/h30": "error",
    "wcag/h32": "error",
    "wcag/h36": "error",
    "wcag/h37": "error",
    "wcag/h67": "error",
    "wcag/h71": "error"
  }
}
