{
  "name": "etceter4",
  "version": "1.0.0",
  "description": "home of the soul.",
  "keywords": [
    "art",
    "website",
    "music",
    "static-site",
    "creative",
    "portfolio"
  ],
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "main": "index.html",
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@eslint/js": "^9.14.0",
    "@playwright/test": "^1.58.2",
    "browser-sync": "^3.0.3",
    "eslint": "^9.14.0",
    "html-validate": "^10.7.0",
    "husky": "^9.0.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "vitest": "^2.0.0"
  },
  "scripts": {
    "dev": "browser-sync start --server --files '*.html, css/*.css, js/*.js' --port 3000",
    "lint": "eslint --config .config/eslint.config.js js/main.js js/page.js js/pageData.js js/images.js js/diary.js js/ogod.js js/analytics.js js/config.js js/audioPlayer.js js/videoPlayer.js js/nameSearch.js js/namingAPI.js js/namingStrategies.js js/loophole.js js/sketch.js js/3d/ui/*.js js/living-pantheon/*.js js/media/**/*.js",
    "lint:fix": "eslint --config .config/eslint.config.js js/main.js js/page.js js/pageData.js js/images.js js/diary.js js/ogod.js js/analytics.js js/config.js js/audioPlayer.js js/videoPlayer.js js/nameSearch.js js/namingAPI.js js/namingStrategies.js js/loophole.js js/sketch.js js/3d/ui/*.js js/living-pantheon/*.js js/media/**/*.js --fix",
    "format": "prettier --config .config/prettierrc.json --ignore-path .config/prettierignore --write '**/*.{js,css,html,md}'",
    "format:check": "prettier --config .config/prettierrc.json --ignore-path .config/prettierignore --check '**/*.{js,css,html,md}'",
    "validate": "npm run lint && npm run format:check",
    "validate:package-lock": "node scripts/validate-package-lock.js",
    "test": "playwright test --config .config/playwright.config.js",
    "test:headed": "playwright test --config .config/playwright.config.js --headed",
    "test:ui": "playwright test --config .config/playwright.config.js --ui",
    "test:unit": "vitest run --config .config/vitest.config.js",
    "test:unit:watch": "vitest --config .config/vitest.config.js",
    "test:unit:coverage": "vitest run --config .config/vitest.config.js --coverage",
    "test:all": "npm run test:unit && npm run test",
    "test:a11y": "playwright test --config .config/playwright.config.js tests/e2e/accessibility.spec.js",
    "validate:html": "html-validate --config .config/htmlvalidate.json index.html",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4-b100m/etceter4.git"
  },
  "author": "et ceter4",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/4-b100m/etceter4/issues"
  },
  "homepage": "https://github.com/4-b100m/etceter4#readme"
}
